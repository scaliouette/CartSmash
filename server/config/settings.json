{
  "aiParsing": {
    "strictMode": true,
    "confidenceThreshold": 0.7,
    "enableAIValidation": true,
    "fallbackToSimple": true,
    "maxProcessingTime": 30,
    "enableCaching": true,
    "batchSize": 50,
    "enableProductValidation": true,
    "enablePriceCheck": false,
    "enableAlternatives": true,
    "showConfidenceScores": true,
    "autoReviewLowConfidence": true,
    "enableSmartSuggestions": true,
    "preferredAI": "chatgpt",
    "enableGeminiValidation": false,
    "apiTimeout": 10,
    "excludePatterns": [
      "cooking instructions",
      "meal descriptions",
      "day names",
      "time references",
      "recipe steps",
      "preparation methods",
      "serving suggestions",
      "optional ingredients"
    ],
    "includePatterns": [
      "quantity + product",
      "measurements",
      "food keywords",
      "grocery terms",
      "brand names",
      "package sizes"
    ],
    "enableAutoCategories": true,
    "customCategories": [],
    "cacheExpiryMinutes": 120,
    "maxConcurrentRequests": 10,
    "exportFormat": "json"
  },
  "productValidation": {
    "enableRealTimeValidation": true,
    "enablePriceChecking": false,
    "enableAvailabilityCheck": false,
    "preferredStores": [
      "kroger"
    ],
    "maxAlternatives": 3,
    "priceThreshold": 200,
    "validationTimeout": 5
  },
  "userInterface": {
    "theme": "light",
    "showAdvancedFeatures": false,
    "enableAnimations": true,
    "autoSaveInterval": 60,
    "showParsingStats": false,
    "enableNotifications": true
  },
  "analytics": {
    "enableTracking": true,
    "enablePerformanceMonitoring": true,
    "enableUserFeedbackCollection": true,
    "retentionDays": 90,
    "enableExport": false
  },
  "system": {
    "environment": "production",
    "logLevel": "info",
    "enableDebugMode": false,
    "enableAPIRateLimiting": true,
    "maxRequestsPerMinute": 100,
    "enableHealthChecks": true
  
  },
  "kroger": {
    "enableValidation": true,
    "cacheProducts": true,
    "cacheDuration": 3600,
    "maxRetries": 3,
    "retryDelay": 1000
  }
}